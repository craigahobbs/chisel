{
    "devDependencies": {
        "ava": "~3.8.0",
        "browser-env": "~3.3.0",
        "eslint": "~7.0.0",
        "esm": "~3.2.0",
        "nyc": "~15.0.0"
    },
    "ava": {
        "timeout": "5m",
        "nodeArguments": [
            "--experimental-modules"
        ],
        "require": [
            "esm"
        ]
    },
    "nyc": {
        "all": true,
        "branches": 100,
        "lines": 100,
        "include": [
            "src",
            "tests"
        ],
        "exclude": []
    }
}
